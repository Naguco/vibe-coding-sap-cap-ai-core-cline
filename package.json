{
  "name": "bookstore-app-ai",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-test": "^0.4.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0",
    "jest": "^30.2.0"
  },
  "scripts": {
    "start": "cds-serve",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "watch-bookstore-app-ai-ui": "cds watch --open bookstoreappaiui/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-customer-shopping-ui": "cds watch --open customershoppingui/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 30000
  },
  "private": true,
  "cds": {
    "requires": {
      "[development]": {
        "auth": {
          "kind": "mocked",
          "users": {
            "customer1": {
              "roles": [
                "customer"
              ]
            },
            "customer2": {
              "roles": [
                "customer"
              ]
            }
          }
        }
      },
      "[production]": {
        "auth": {
          "kind": "xsuaa"
        }
      },
      "[hybrid]": {},
      "html5-repo": true,
      "destinations": true,
      "workzone": true
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/customer-shopping-ui"
  ]
}
