{
  "name": "bookstore-app-ai",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-test": "^0.4.0",
    "@cap-js/sqlite": "^2",
    "jest": "^30.2.0",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "watch-bookstore-app-ai-ui": "cds watch --open bookstoreappaiui/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 30000
  },
  "private": true,
  "cds": {
    "requires": {
      "auth": {
        "kind": "mocked",
        "users": {
          "customer1": {
            "roles": [
              "customer"
            ]
          }
        }
      }
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/bookstore-app-ai-ui"
  ]
}
